<?xml version="1.0" encoding="UTF-8"?>
<beans  xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context="http://www.springframework.org/schema/context"
   xmlns:p="http://www.springframework.org/schema/p"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context 
    http://www.springframework.org/schema/context/spring-context.xsd">

<bean  name="student"  class="com.collection.Student" p:name="farid">
<property name="Book">
<list  >
<value>Java Book</value>
<value>Python Book</value>
<value>Scala Book</value>
<null/>
<null/>
</list>
</property>
<property name="Phone">
<set>
<value>Samsung</value>
<value>Apple</value>
<value>Nokia</value>
</set>
</property>
<property name="course">
<map>
<entry  key="course1" value="Java" />
<entry key="course2" value="Python"/>
<entry  key="course3"  value="Scala"/>
</map>
</property>

</bean> </beans>
